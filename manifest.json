{
    "manifest_version": 3,
    "name": "SOLS Timetable to ICS",
    "version": "1.0.0",
    "description": "Export your UOW SOLS timetable to an ICS calendar file",
    "permissions": [
        "activeTab",
        "downloads"
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": [
                "https://solss.uow.edu.au/sid/sols_tutorial_enrolment.my_timetable*"
            ],
            "js": [
                "calendar.js",
                "content.js"
            ]
        }
    ]
}